{
  "name": "@skybrush/viewer",
  "version": "2.7.0",
  "description": "Viewer for Skybrush drone shows",
  "main": "launcher.bundle.js",
  "scripts": {
    "bundle": "rimraf build && cross-env NODE_ENV=production webpack --mode=production --progress --config webpack/dist.config.js",
    "i18n": "i18next",
    "lint": "xo",
    "postinstall": "patch-package",
    "analyze": "webpack --config webpack/browser.config.js --progress --json >stats.json",
    "start": "cross-env SKYBRUSH_VARIANT=demo webpack serve --config webpack/browser.config.js --progress",
    "start:electron": "webpack serve --config webpack/electron.config.js --progress",
    "start:public": "webpack serve --config webpack/browser.config.js --progress --host 0.0.0.0",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:skybrush-io/viewer.git"
  },
  "keywords": [],
  "author": "CollMot Robotics Ltd",
  "license": "GPL-3.0-or-later",
  "private": true,
