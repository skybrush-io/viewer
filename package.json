    "electron-store": "^8.1.0",
    "electron-util": "^0.17.2",
    "electron-window-state": "^5.0.3",
    "esm": "^3.2.25",
    "express": "^4.18.2",
    "https-browserify": "^1.0.0",
    "js-base64": "^3.7.5",
    "ky": "^1.1.3",
    "lodash-es": "^4.17.21",
    "natural-orderby": "^3.0.1",
    "node-ssdp": "^4.0.1",
    "process": "^0.11.10",
